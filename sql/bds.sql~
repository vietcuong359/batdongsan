  CONSTRAINT `fk_Tin_phuong_xa_Tin1`
    FOREIGN KEY (`ma_tin` )
    REFERENCES `bds`.`Tin` (`ma_tin` )
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `fk_Tin_Phuong_xa_Phuong_xa1`
    FOREIGN KEY (`ma_phuong_xa` )
    REFERENCES `bds`.`Phuong_xa` (`ma_phuong_xa` )
    ON DELETE RESTRICT
    ON UPDATE CASCADE)
ENGINE = InnoDB;


