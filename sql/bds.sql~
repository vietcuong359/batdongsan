    REFERENCES `bds`.`Quan_huyen` (`ma_quan_huyen` )
    ON DELETE RESTRICT
    ON UPDATE CASCADE)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `bds`.`Tin_Phuong_xa`
-- -----------------------------------------------------
