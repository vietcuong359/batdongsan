    FOREIGN KEY (`ma_loai_vp` )
    REFERENCES `bds`.`Loai_van_phong` (`ma_loai_vp` )
    ON DELETE RESTRICT
    ON UPDATE CASCADE)
ENGINE = InnoDB;


